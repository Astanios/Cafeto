{
  "name": "cafeto-node-seed",
  "version": "0.1.0",
  "description": "Cafeto Node JS seed project",
  "main": "src/app.js",
  "scripts": {
    "start": "node ./src/app",
    "dev": "nodemon ./src/app",
    "test": "nyc mocha test --recursive && nyc report --reporter=lcov",
    "lint": "node ./node_modules/eslint/bin/eslint.js src",
    "migration:up": "node_modules/.bin/sequelize db:seed:all",
    "migration:down": "node_modules/.bin/sequelize db:seed:undo",
    "migration:generate": "node_modules/.bin/sequelize seed:generate --name unamed-file"
  },
  "keywords": [
    "Node",
    "JS",
    "Restify",
    "Sequelize"
  ],
  "author": "Cafeto Software",
  "license": "Proprietary",
  "dependencies": {
    "bottlejs": "1.7.1",
    "dotenv": "6.2.0",
    "jsonwebtoken": "8.4.0",
    "jwks-rsa": "^1.8.1",
    "le_node": "1.8.0",
    "meld": "1.3.2",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "pg-hstore": "2.3.2",
    "restify": "7.5.0",
    "restify-cors-middleware": "1.1.1",
    "restify-jwt-community": "1.0.9",
    "restify-router": "0.5.1",
    "sequelize": "6.3.5",
    "sequelize-cli": "5.4.0",
    "tcomb-form-native": "^0.6.20"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "cross-env": "5.2.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.9",
    "nyc": "13.1.0",
    "sinon": "7.2.2",
    "standard": "12.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
